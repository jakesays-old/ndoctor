<?xml version="1.0" encoding="utf-8" ?>
<Nini>
  <Section Name="GoogleCalendar">
    <!-- Indicates whether nDoctor link its appointments with Google Calendar -->
    <Key Name="IsGoogleCalendarEnabled" Value="False"/>
    <!-- By default, nDoctor uses only the value of the default calendar. 
       More explanations here:  http://code.google.com/p/ndoctor/wiki/UserManual?ts=1352912575&updated=UserManual#How_to_configure_a_calendar_to_be_the_one_nDoctor_uses
    -->
    <Key Name="BindedCalendar" Value="https://www.google.com/calendar/feeds/default/private/full"/>
    <!-- The user name to connect to Google Calendar -->
    <Key Name="UserName" Value=""/>
    <!-- Encrypted password, do not modify, it is done by nDoctor -->
    <Key Name="Password" Value=""/>
  </Section>
</Nini>

